{
  "name": "daily-index-tracker",
  "version": "0.1.0",
  "description": "Daily Index Tracker - Crypto market monitoring system",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "npm run dev:frontend",
    "build": "npm run build:frontend",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "dev:frontend": "cd src/admin-console && npm run dev",
    "build:frontend": "cd src/admin-console && npm run build",
    "deploy": "wrangler deploy --env production",
    "deploy:dev": "wrangler deploy --env development",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "secrets:configure": "./scripts/deployment/configure-secrets.sh",
    "secrets:validate": "./scripts/deployment/validate-secrets.sh",
    "validate": "wrangler validate",
    "logs": "wrangler tail --env production",
    "logs:dev": "wrangler tail --env development",
    "logs:staging": "wrangler tail --env staging",
    "verify:real": "node verify-real-n8n.js",
    "verify:real:test": "node verify-real-n8n.js test",
    "verify:real:webhooks": "node verify-real-n8n.js webhooks",
    "verify:real:connectivity": "node verify-real-n8n.js connectivity",
    "verify:real:upload": "node verify-real-n8n.js upload-instructions",
    "test:ingestion": "node test-ingestion-with-real-data.js",
    "test:n8n-flow": "node test-n8n-to-ingestion.js",
    "test:timeseries": "node test-timeseries-analysis.js all",
    "test:timeseries:generate": "node test-timeseries-analysis.js generate",
    "test:timeseries:validate": "node test-timeseries-analysis.js validate",
    "test:timeseries:analyze": "node test-timeseries-analysis.js analyze",
    "test:kv-analysis": "node test-actual-kv-analysis.js all",
    "test:kv-analysis:fetch": "node test-actual-kv-analysis.js fetch",
    "test:kv-analysis:kpi": "node test-actual-kv-analysis.js test-kpi",
    "test:real-analysis": "node test-real-data-analysis.js all",
    "test:real-analysis:convert": "node test-real-data-analysis.js convert",
    "test:real-analysis:analyze": "node test-real-data-analysis.js analyze",
    "test:real-analysis:test": "node test-real-data-analysis.js test",
    "demo:analysis": "node demo-analysis-usage.js"
  },
  "keywords": [
    "crypto",
    "monitoring",
    "cloudflare",
    "n8n",
    "serverless",
    "kpi",
    "analytics"
  ],
  "author": "Daily Index Tracker Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.0"
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "wrangler": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pohlipit23/metric_stream.git"
  }
}
